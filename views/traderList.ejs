<%- include('partials/traderHeader.ejs'); %>
    <nav class="navbar navbar-expand-lg navbar-light border-primary border-bottom">
        <div class="container-fluid">
            <a class="navbar-brand text-primary" href="#"><b>社員一覧</b></a>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active text-primary" aria-current="page" href="/master"><b>Home</b></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active text-primary" aria-current="page" href="/traderentry"><b>社員登録</b></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container" style="margin-top: 30px;">
        <table class="table table-bordered table-hover text-center">
            <thead class="">
                <tr>
                    <th>No</th>
                    <th>社員コード</th>
                    <th>名前</th>
                    <th>メールアドレス</th>
                    <th>電話番号</th>
                    <th>住所</th>
                </tr>
            </thead>
            <tbody>
                <% for (var i = 0; i < values.length; i++ ) { %>
                    <tr>
                        <td>
                            <%= i+1 %>
                        </td>
                        <td>
                            <%= values[i].admin_id %>
                        </td>
                        <td>
                            <%= values[i].last_name %>
                                <%= values[i].first_name %>
                        </td>
                        <td>
                            <%= values[i].mail %>
                        </td>
                        <td>
                            <%= values[i].call_number %>
                        </td>
                        <td>
                            <%= values[i].address %>
                        </td>
                        <% } %>
                    </tr>
            </tbody>
        </table>
    </div>