    <form class="box" action="/traderuser" method="post">
        <h1>Entry</h1>
        <input type="text" name="adminid" placeholder="adminid" required>
        <input type="text" name="lastname" placeholder="性" required>
        <input type="text" name="firstname" placeholder="名" required>
        <input type="text" name="hlast" placeholder="セイ" required>
        <input type="text" name="hfirst" placeholder="メイ" required>
        <input type="text" name="phone" placeholder="電話番号" required>
        <input type="text" name="mail" placeholder="メールアドレス" required>
        <input type="text" name="birthday" placeholder="YYYYmmdd" required>
        <input type="text" name="address" placeholder="住所" required>
        <input type="hidden" name="flg" value="0">
        <input type="submit" name="entry" value="登録">
    </form>
